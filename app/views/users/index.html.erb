<div class="container">
 <div class="row">
   <div class="col-md-3">
     <%= render 'indexlist', user: @users %>
   </div>
   <div class="col-md-8 offset-md-1">
     <h2>User</h2>
     <table class="table">
       <thead>
         <tr>
           <th>Image</th>
           <th>Name</th>
           <th></th>
         </tr>
       </thead>
       <tbody>
         <tr>
           <% @users.each do |user| %>
           <td><%= image_tag user.get_profile_image(80,80) %></td>
           <td><%= user.name %></td>
           <td><%= link_to 'Show', user_path(user.id) %></td>
         </tr>
        </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>